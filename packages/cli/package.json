{
  "name": "roomy-cli",
  "version": "1.0.0",
  "description": "Roomy CLI - Send messages to Roomy spaces from the command line",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "bin": {
    "roomy": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "roomy",
    "cli",
    "chat",
    "messaging",
    "atproto",
    "jazz"
  ],
  "author": "Roomy Team",
  "license": "MIT",
  "dependencies": {
    "@atproto/oauth-client-node": "^0.3.1",
    "@atproto/api": "^0.14.9",
    "@atproto-labs/simple-store": "^0.2.0",
    "jazz-tools": "^0.14.21",
    "@muni-town/leaf": "^0.2.0-preview.22",
    "@muni-town/leaf-sync-ws": "^0.1.0-preview.7",
    "commander": "^11.0.0",
    "inquirer": "^9.2.0",
    "chalk": "^5.3.0",
    "open": "^9.1.0",
    "ws": "^8.14.0",
    "@inquirer/prompts": "^3.0.0",
    "express": "^4.18.0",
    "node-fetch": "^3.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/inquirer": "^9.0.0",
    "@types/ws": "^8.5.0",
    "@types/express": "^4.17.0",
    "typescript": "^5.0.0",
    "tsx": "^4.0.0"
  }
}